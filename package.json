{
  "name": "alexjray.com",
  "version": "1.0.0",
  "engines": {
    "node": "7.5.0",
    "npm": "4.1.2"
  },
  "description": "Portolio for Alexander J Ray",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_PATH=./src NODE_ENV=development node ./src/server/server.babel.js",
    "staging": "NODE_PATH=./src NODE_ENV=production npm run prod",
    "prod": "node src/server/server.babel.js",
    "deploy": "npm run build && git add --all . && git commit -m 'Adds new build files' && npm run deploy:assets && git push production master",
    "deploy:assets": "gulp --require babel-register --gulpfile ./Gulpfile.js upload",
    "build": "npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=production webpack --config ./webpack/server.babel.js",
    "build:client": "NODE_ENV=production webpack --config ./webpack/prod.babel.js"
  },
  "author": "Alexander J Ray",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.5",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.0",
    "gulp": "^3.9.1",
    "gulp-awspublish": "^3.3.0",
    "gulp-s3-upload": "^1.6.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "postcss-loader": "^1.3.1",
    "querystring": "^0.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0"
  },
  "dependencies": {
    "autoprefixer": "^6.7.5",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "dotenv": "^4.0.0",
    "dotenv-expand": "^0.1.0",
    "emoji-favicon": "^0.3.0",
    "express": "^4.14.1",
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.4.2",
    "react-dom-stream": "^0.5.1",
    "react-player": "^0.14.3",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-immutablejs": "0.0.8",
    "viewport-units-buggyfill": "^0.6.0",
    "webpack": "^2.2.1"
  }
}
